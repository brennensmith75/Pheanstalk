{
    "name": "pda/pheanstalk",
    "type": "library",
    "description": "PHP client for beanstalkd queue",
    "keywords": ["beanstalkd"],
    "homepage": "https://github.com/pda/pheanstalk",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Annesley",
            "email": "paul@annesley.cc",
            "homepage": "http://paul.annesley.cc/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Pheanstalk\\": "src/"
        }
    },
    "scripts": {
        "phpunit": "docker-compose run --rm phpunit",
        "test": [
            "@composer install",
            "@phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
